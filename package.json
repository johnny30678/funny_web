{
  "name": "funny_web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:dev": "npm run pages:build && npx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat --port 3001",
    "pages:deploy": "npm run pages:build && npx wrangler pages deploy .vercel/output/static"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "next": "13.4.2"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "tailwindcss": "^3.3.0",
    "postcss": "^8.4.14",
    "autoprefixer": "^10.4.14",
    "eslint": "^8",
    "eslint-config-next": "13.4.2",
    "@cloudflare/next-on-pages": "^1.0.0",
    "wrangler": "^3.22.1"
  }
}
